<h1>Create</h1>

{{#if isCreator }}
	{{allow-open useGeoPosition=useGeoPosition}}
{{/if}}

{{joined-player-list players=model.players isCreator=isCreator action="kickPlayer"}}

{{#if isCreator }}
	<div>
	    {{invite-player}}
	    {{start-game action="createGame"}}
	</div>
{{else}}
	<div>
		{{back-button action="back"}}
	</div>
{{/if}}

{{outlet}}
